{
  "python:3.8": {
    "prefixes": [
      "py",
      "python",
      "gyp"
    ],
    "file": "main.py",
    "command": "python main.py"
  },
  "c": {
    "prefixes": [
      "c",
      "h"
    ],
    "file": "main.c",
    "command": "gcc main.c -o main && ./main"
  },
  "haskell": {
    "prefixes": [
      "haskell",
      "hs"
    ],
    "file": "Main.hs",
    "command": "ghc Main.hs && ./Main"
  },
  "java": {
    "prefixes": [
      "java",
      "jsp"
    ],
    "file": "Main.java",
    "command": "javac Main.java && java Main"
  },
  "cpp": {
    "prefixes": [
      "cpp",
      "hpp",
      "cc",
      "hh",
      "c++",
      "h++",
      "cxx",
      "hxx"
    ],
    "file": "main.cpp",
    "command": "g++ main.cpp -o main && ./main"
  },
  "rust": {
    "prefixes": [
      "rust",
      "rs"
    ],
    "file": "main.rs",
    "command": "rustc main.rs && ./main"
  },
  "csharp": {
    "prefixes": [
      "csharp",
      "cs"
    ],
    "file": "main.cs",
    "command": "mcs -out:main.exe main.cs && mono main.exe"
  },
  "go": {
    "prefixes": [
      "go",
      "golang"
    ],
    "file": "main.go",
    "command": "go run main.go"
  },
  "js": {
    "prefixes": [
      "js",
      "jsx",
      "javascript"
    ],
    "file": "main.js",
    "command": "node main.js"
  },
  "clojure": {
    "prefixes": [
      "clojure",
      "clj"
    ],
    "file": "main.clj",
    "command": "clj main.clj"
  },
  "fsharp": {
    "prefixes": [
      "fsharp",
      "fs"
    ],
    "file": "main.fsx",
    "command": "fsharpi main.fsx"
  },
  "kotlin": {
    "prefixes": [
      "kotlin",
      "kt"
    ],
    "file": "main.kt",
    "command": "kotlinc main.kt -include-runtime -d main.jar && java -jar main.jar"
  },
  "scala": {
    "prefixes": [
      "scala"
    ],
    "file": "Main.scala",
    "command": "scala -nc Main.scala"
  },
  "lisp": {
    "prefixes": [
      "lisp"
    ],
    "file": "main.lsp",
    "command": "sbcl --script main.lsp"
  },
  "bash": {
    "prefixes": [
      "bash",
      "sh",
      "zsh"
    ],
    "file": "main.bash",
    "command": "bash main.bash"
  },
  "scheme": {
    "prefixes": [
      "scheme"
    ],
    "file": "main.scm",
    "command": "scheme --quiet < main.scm"
  },
  "ruby": {
    "prefixes": [
      "ruby",
      "rb",
      "gemsp",
      "ec",
      "podspec",
      "thor",
      "irb"
    ],
    "file": "main.rb",
    "command": "ruby main.rb"
  },
  "r": {
    "prefixes": [
      "r"
    ],
    "file": "main.r",
    "command": "Rscript main.r"
  },
  "php": {
    "prefixes": [
      "php",
      "php3",
      "php4",
      "php5",
      "php6",
      "php7"
    ],
    "file": "main.php",
    "command": "php main.php"
  },
  "perl": {
    "prefixes": [
      "perl",
      "pl",
      "pm"
    ],
    "file": "main.pearl",
    "command": "pearl main.pl"
  },
  "ocaml": {
    "prefixes": [
      "ocaml",
      "ml"
    ],
    "file": "main.ml",
    "command": "ocamlc -o main main.ml && ./main"
  },
  "lua": {
    "prefixes": [
      "lua"
    ],
    "file": "main.lua",
    "command": "lua main.lua"
  },
  "typescript": {
    "prefixes": [
      "typescript",
      "ts"
    ],
    "file": "main.ts",
    "command": "tsc main.ts && node main.js"
  },
  "dart": {
    "prefixes": [
      "dart"
    ],
    "file": "main.dart",
    "command": "dart main.dart"
  },
  "swift": {
    "prefixes": [
      "swift"
    ],
    "file": "main.swift",
    "command": "swiftc -o main main.swift && ./main"
  }
}
